{% extends './base.twig' %}

{% block body_class %}menus-page{% endblock %}

{% block header %}
	{% include './partials/header.twig' with { "current_page": "menus" } %}
{% endblock %}

{% block content %}

  {% include './partials/left-blob-svg.twig' with {"class": "parallax  blob-left", "data_speed": "0.4"} %}

	{% include './partials/menu.twig' with {"data": menus_data.menu_block} %}

	{% include './partials/single-asset.twig' with {"data": menus_data.single_asset} %}

	{% include './partials/text-with-image.twig' with {"data": menus_data.block_1} %}
	{% include './partials/text-with-image.twig' with {"data": menus_data.block_2} %}

	{% include './partials/rich-links-ticker.twig' %}

  {% include './partials/right-blob-svg.twig' with {"class": "parallax", "data_speed": "0.8"} %}

{% endblock %}

{% block footer %}
	{% include './partials/footer.twig' %}
{% endblock %}
